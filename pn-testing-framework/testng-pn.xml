	</listeners>
	<test name="fridges-sorting" enabled="false">
		<classes>
			<class name="com.epam.davydov.pn.tests.ProductsSorting" />
		</classes>
	</test>
	<test name="microwave-comparison" enabled="false">
		<classes>
			<class name="com.epam.davydov.pn.tests.ProductsComparison" />
		</classes>
	</test>
	<test name="bakery-filter" enabled="false">
		<classes>
			<class name="com.epam.davydov.pn.tests.FilterFunctioning" />
		</classes>
	</test>
	<test name="washers-price-filter" enabled="false">
		<classes>
			<class name="com.epam.davydov.pn.tests.PriceFilter" />
		</classes>
	</test>
	<test name="bakery-manufacturer-filter" enabled="false">
		<classes>
			<class name="com.epam.davydov.pn.tests.ManufacturerFilter" />
